<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.facile2soutenir.mobile" android-versionCode="1" version="1.0.5" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Facile2Soutenir</name>
    <description>
         Publication sur le store
    </description>
    <author email="contact@facile2soutenir.fr" href="http://www.facile2soutenir.fr">
        Team Facile2Soutenir
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ee6e73" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin spec="https://github.com/EddyVerbruggen/phonegap-facebook-plugin" source="git">   
        <param name="APP_ID" value="204764659934740" />
        <param name="APP_NAME" value="Connexion Facile2Soutenir" />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
